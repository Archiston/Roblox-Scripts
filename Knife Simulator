esp.MouseButton1Down:connect(function()
	-- Farewell Infortality.
	-- Version: 2.82
	-- Instances:
	local ScreenGui = Instance.new("ScreenGui")
	local main = Instance.new("Frame")
	local makhacks = Instance.new("TextLabel")
	local buttonhack = Instance.new("TextButton")
	--Properties:
	ScreenGui.Parent = game.CoreGui
	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	main.Name = "main"
	main.Parent = ScreenGui
	main.BackgroundColor3 = Color3.new(1, 1, 1)
	main.Position = UDim2.new(-1.13248825e-05, 0, 0.692621291, 0)
	main.Size = UDim2.new(0, 148, 0, 123)
	main.Style = Enum.FrameStyle.DropShadow

	makhacks.Name = "makhacks"
	makhacks.Parent = main
	makhacks.BackgroundColor3 = Color3.new(0, 0, 0)
	makhacks.BackgroundTransparency = 5
	makhacks.Position = UDim2.new(0.126388192, 0, 0.056356363, 0)
	makhacks.Size = UDim2.new(0, 99, 0, 36)
	makhacks.Font = Enum.Font.SourceSans
	makhacks.Text = "MakHACKs"
	makhacks.TextColor3 = Color3.new(1, 0, 0)
	makhacks.TextScaled = true
	makhacks.TextSize = 14
	makhacks.TextWrapped = true

	buttonhack.Name = "button hack"
	buttonhack.Parent = main
	buttonhack.BackgroundColor3 = Color3.new(0, 0.658824, 0.658824)
	buttonhack.BackgroundTransparency = 0.5
	buttonhack.Position = UDim2.new(0.0685841292, 0, 0.452131331, 0)
	buttonhack.Size = UDim2.new(0, 115, 0, 45)
	buttonhack.Font = Enum.Font.SourceSans
	buttonhack.Text = "Aimbot"
	buttonhack.TextColor3 = Color3.new(0.976471, 1, 0.686275)
	buttonhack.TextScaled = true
	buttonhack.TextSize = 14
	buttonhack.TextWrapped = true
	buttonhack.MouseButton1Down:connect(function()
		local plrs = game:service("Players")
		local ms = plrs.LocalPlayer:GetMouse()
		local nearest = function()
			local plr,dist = nil,5000
			for k,l in pairs(plrs:GetPlayers()) do
				local pos = l.Character:GetPrimaryPartCFrame().p
				local len = (ms.Hit.p - pos).Magnitude
				if len <= dist then
					plr = l
					dist = len
				end
			end
			return plr.Character:GetPrimaryPartCFrame().p
		end
		workspace.CurrentCamera.trash.ChildAdded:connect(function(c)
			c.CFrame = CFrame.new(nearest())
		end)
	end)

	function SCRIPT_DLQI88_FAKESCRIPT() -- makhacks.Script 
		local script = Instance.new('Script')
		script.Parent = makhacks
		while wait(0.2)do
			script.Parent.TextColor3 = Color3.fromHSV(math.random(100)/100,1,1)

		end

	end
	coroutine.resume(coroutine.create(SCRIPT_DLQI88_FAKESCRIPT))
end)

esp.Name = "esp"
esp.Parent = main
esp.BackgroundColor3 = Color3.fromRGB(255, 0, 255)
esp.Position = UDim2.new(0.562845588, 0, 0.278260857, 0)
esp.Size = UDim2.new(0, 160, 0, 60)
esp.Font = Enum.Font.SourceSans
esp.Text = "Esp"
esp.TextColor3 = Color3.fromRGB(0, 0, 0)
esp.TextScaled = true
esp.TextSize = 25.000
esp.TextWrapped = true
esp.MouseButton1Down:connect(function()

	--- Tut

	local esp_settings = { ---- table for esp settings 
		textsize = 8,
		colour = 255,255,255
	}

	local gui = Instance.new("BillboardGui")
	local esp = Instance.new("TextLabel",gui) ---- new instances to make the billboard gui and the textlabel



	gui.Name = "Cracked esp"; ---- properties of the esp
	gui.ResetOnSpawn = false
	gui.AlwaysOnTop = true;
	gui.LightInfluence = 0;
	gui.Size = UDim2.new(1.75, 0, 1.75, 0);
	esp.BackgroundColor3 = Color3.fromRGB(255, 255, 255);
	esp.Text = ""
	esp.Size = UDim2.new(0.0001, 0.00001, 0.0001, 0.00001);
	esp.BorderSizePixel = 4;
	esp.BorderColor3 = Color3.new(esp_settings.colour)
	esp.BorderSizePixel = 0
	esp.Font = "GothamSemibold"
	esp.TextSize = esp_settings.textsize
	esp.TextColor3 = Color3.fromRGB(esp_settings.colour) -- text colour

	game:GetService("RunService").RenderStepped:Connect(function() ---- loops faster than a while loop :)
		for i,v in pairs (game:GetService("Players"):GetPlayers()) do
			if v ~= game:GetService("Players").LocalPlayer and v.Character.Head:FindFirstChild("Cracked esp")==nil and v.TeamColor ~= game:GetService("Players").LocalPlayer.TeamColor then -- craeting checks for team check, local player etc
				esp.Text = "{"..v.Name.."}"
				gui:Clone().Parent = v.Character.Head
			end
		end
	end)
end)
	
