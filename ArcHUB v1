Arsenal.MouseButton1Down:connect(function()
	pcall(function()
		local espcolor = Color3.fromRGB(140, 69, 102)
		local wallhack_esp_transparency = .4
		local gui_hide_button = {Enum.KeyCode.LeftControl, "h"}
