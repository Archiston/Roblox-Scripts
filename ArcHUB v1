	if Settings["Destroy Client Updater"] == true then
		for _,LaggyRemoteEvent in pairs (game.Players.LocalPlayer.PlayerGui.Events:GetChildren()) do
			LaggyRemoteEvent:Destroy()
		end
	end

	local CheckDeleters = function(Info)
		for _,Delete in pairs(DeletePets) do
			if string.lower(tostring(Delete)) == string.lower(Directory.Pets[Info].DisplayName) then
				return true
			end
			if string.lower(tostring(Delete)) == string.lower(Directory.Pets[Info].ReferenceName) then
				return true
			end
		end
	end

	local GetStats = function()
		return workspace["__REMOTES"]["Core"]["Get Stats"]:InvokeServer()
	end

	local Triple_Egg_Open

	local BuyEgg = function()
		if Triple_Egg_Open == true then
			return workspace["__REMOTES"]["Game"]["Shop"]:InvokeServer("Buy", "Eggs", Settings["Auto Egg"]["Egg Tier"], true)
		else
			return workspace["__REMOTES"]["Game"]["Shop"]:InvokeServer("Buy", "Eggs", Settings["Auto Egg"]["Egg Tier"])
		end
	end

	local PrintPetInfo = function(PetInfo, PetID)
		warn(tostring("Pet Name: " .. Directory.Pets[PetInfo].DisplayName .. " - ID: " .. PetID))
	end

	local DeletePet = function(N, PetID)
		warn(tostring("Deleting Pet: " .. Directory.Pets[N].DisplayName .. " - ID: " .. PetID))
		workspace["__REMOTES"]["Game"]["Inventory"]:InvokeServer("Delete", PetID)
	end

	local Success,Message = workspace["__REMOTES"]["Game"]["Shop"]:InvokeServer("Buy", "Eggs", "Tier 1", true)
	if Success == false and tostring(Message) == "You need to buy the triple egg open gamepass" then
		Triple_Egg_Open = false
	elseif Success == true then
		for PetNumber,PetInfo in ipairs (Message) do
			for rewardNum,rewardData in ipairs (PetInfo) do
				if rewardData then
					Triple_Egg_Open = true
					workspace["__REMOTES"]["Game"]["Inventory"]:InvokeServer("Delete", rewardData.id)
				end
			end
		end
	end

	local Main = (function()
		repeat
			local Stats = GetStats()
			wait()
			if #Stats.Save.Pets < Stats.Save.PetSlots then
				Settings["Auto Egg"]["Egg Purchase Amount"] = Settings["Auto Egg"]["Egg Purchase Amount"] - 1
				local EggPurchase,EggPurchaseInfo = BuyEgg()
				if EggPurchase == true and EggPurchaseInfo ~= "You need more to buy this!" and EggPurchaseInfo ~= nil then
					for PetNumber,PetInfo in pairs (EggPurchaseInfo) do
						for rewardNum,rewardData in ipairs (PetInfo) do
							if rewardData then
								local PetN = rewardData.n
								local PetID = rewardData.id
								PrintPetInfo(PetN, PetID)
								if CheckDeleters(PetN) == true then
									if rewardData.r then
										if Settings["Auto Egg"]["Keep All Rainbow Pets"] ~= true then
											DeletePet(PetN, PetID)
										end
									else
										DeletePet(PetN, PetID)
									end
								end
							end
						end
					end
				end
			end
		until
		Settings["Auto Egg"]["Egg Purchase Amount"] <= 0
	end)

	local AutoCombineCheck = (function()
		local Stats = GetStats()
		local GoldTable = {}
		local RainbowTable = {}
		local DarkMatterTable = {}
		for _,Pet in ipairs(Stats.Save.Pets) do
			if Settings["Auto Combine"]["Gold"] == true and Pet.g ~= true and Pet.r ~= true and Pet.dm ~= true then
				if GoldTable[tostring(Pet.n)] then
					GoldTable[tostring(Pet.n)] = GoldTable[tostring(Pet.n)] + 1
				else
					GoldTable[tostring(Pet.n)] = 1
				end
			elseif Settings["Auto Combine"]["Rainbow"] == true and Pet.g == true and Pet.r ~= true and Pet.dm ~= true then
				if RainbowTable[tostring(Pet.n)] then
					RainbowTable[tostring(Pet.n)] = RainbowTable[tostring(Pet.n)] + 1
				else
					RainbowTable[tostring(Pet.n)] = 1
				end
			elseif Settings["Auto Combine"]["Dark Matter"] == true and Pet.g ~= true and Pet.r == true and Pet.dm ~= true then
				if DarkMatterTable[tostring(Pet.n)] then
					DarkMatterTable[tostring(Pet.n)] = DarkMatterTable[tostring(Pet.n)] + 1
				else
					DarkMatterTable[tostring(Pet.n)] = 1
				end
			end
		end
		if Settings["Auto Combine"]["Gold"] == true then
			for PetN,Amount in pairs (GoldTable) do
				if Amount >= 10 then
					for _,Pet in ipairs(Stats.Save.Pets) do
						if tostring(Pet.n) == tostring(PetN) and Pet.g ~= true and Pet.r ~= true and Pet.dm ~= true and Amount >= 10 then
							if CheckDeleters(PetN) ~= true then
								workspace["__REMOTES"]["Game"]["Golden Pets"]:InvokeServer(Pet.id)
								Amount = 0
							end
						end
					end
				end
			end
		end
		if Settings["Auto Combine"]["Rainbow"] == true then
			for PetN,Amount in pairs (RainbowTable) do
				if Amount >= 7 then
					for _,Pet in ipairs(Stats.Save.Pets) do
						if tostring(Pet.n) == tostring(PetN) and Pet.g == true and Pet.r ~= true and Pet.dm ~= true and Amount >= 7 then
							if CheckDeleters(PetN) ~= true then
								workspace["__REMOTES"]["Game"]["Rainbow Pets"]:InvokeServer(Pet.id)
								Amount = 0
							end
						end
					end
				end
			end
		end
		if Settings["Auto Combine"]["Dark Matter"] == true then
			for PetN,Amount in pairs (DarkMatterTable) do
				if Amount >= 5 then
					for _,Pet in ipairs(Stats.Save.Pets) do
						if tostring(Pet.n) == tostring(PetN) and Pet.g ~= true and Pet.r == true and Pet.dm ~= true and Amount >= 5 then
							if CheckDeleters(PetN) ~= true then
								workspace["__REMOTES"]["Game"]["Dark Matter Pets"]:InvokeServer(Pet.id)
								Amount = 0
							end
						end
					end
				end
			end
		end
	end)

	if Settings["Auto Egg"]["Auto Egg Purchase Enabled"] == true then
		for i=1,Settings["Auto Egg"]["Threads"] do
			spawn(function()
				Main()
			end)
		end
	end

	if Settings["Auto Combine"]["Enabled"] == true then
		for i=1,Settings["Auto Combine"]["Threads"] do
			spawn(function()
				while wait() do
					AutoCombineCheck()
				end
			end)
		end
	end
end)

